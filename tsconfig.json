{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "lib",
    "declaration": true,
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "pretty": true,
  },
  "exclude": [
    "example/docs",
    "example",
    "docs"
  ]
}