{
  "name": "react-native-wear",
  "version": "1.0.10",
  "description": "Description",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-respondr-react-native-wear-module.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "dotenv release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "wear",
    "react-native-wear",
    "wear-os"
  ],
  "repository": "https://github.com/darknet-00/react-native-wear-communication-module",
  "author": {
    "name": "Nikola Krezeski",
    "email": "nkrezeski1@gmail.com",
    "url": "https://github.com/darknet-00"
  },
  "contributors": [
    {
      "name": "Ivan Janjikj",
      "email": "ijanjikj@banzae.dev"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beaverfy/react-native-wear/issues"
  },
  "homepage": "https://reactnativewear.vercel.app/",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.4.3",
    "@react-native-community/eslint-config": "^3.2.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/jest": "^26.0.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "0.72.6",
    "commitlint": "^18.4.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "pod-install": "^0.1.39",
    "react-native-builder-bob": "^0.23.2",
    "release-it": "^17.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
